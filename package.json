{
  "name": "@belkajo/motion-engine",
  "version": "0.1.2",
  "description": "Tiny stateful animation sequence engine on top of framer-motion.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "framer-motion",
    "animation",
    "state-machine",
    "sequence"
  ],
  "author": "@belkajo",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18",
    "framer-motion": ">=11"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.4.1"
}
